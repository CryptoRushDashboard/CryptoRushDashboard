{
  "name": "cryptostorm-www-github",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build-css": "stylus src/styles/main.styl -o static/css",
    "watch-css": "stylus src/styles/main.styl -o static/css -w",
    "clean": "rm -rf static/css && mkdir -p static/css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon server -e js,pug",
    "start": "node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CryptoRushDashboard/CryptoRushDashboard.github.io.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CryptoRushDashboard/CryptoRushDashboard.github.io/issues"
  },
  "homepage": "https://github.com/CryptoRushDashboard/CryptoRushDashboard.github.io#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pug": "^2.0.0-rc.4"
  }
}
